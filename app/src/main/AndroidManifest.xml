        <activity android:name=".Song4Activity"
            android:label="@string/Song_4" />

        <activity android:name=".Song5Activity"
            android:label="@string/Song_5" />

        <activity android:name=".Song6Activity"
            android:label="@string/Song_6" />

        <activity android:name=".Song7Activity"
            android:label="@string/Song_7" />

        <activity android:name=".Song8Activity"
            android:label="@string/Song_8" />

        <activity android:name=".Song9Activity"
            android:label="@string/Song_9" />


